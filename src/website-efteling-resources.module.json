{
    "$schema": "../../.sitecore/schemas/ModuleFile.schema.json",
    "namespace": "Efteling.Resources",
    "items": {
        "includes": [
            {
                "name": "templates",
                "path": "/sitecore/templates/Project/efteling",
                "allowedPushOperations": "CreateUpdateAndDelete"
            },
            {
                "name": "layouts",
                "path": "/sitecore/layout/Layouts/Project/efteling",
                "allowedPushOperations": "CreateUpdateAndDelete"
            },       
            {
                "name": "renderings",
                "path": "/sitecore/layout/Renderings/Project/efteling",
                "allowedPushOperations": "CreateUpdateAndDelete"
            },     
            {
                "name": "efteling automation rules definition elements",
                "path": "/sitecore/system/Settings/Rules/Definitions/Elements/Efteling Automation",
                "allowedPushOperations": "CreateUpdateAndDelete"
            },
            {
                "name": "item saved rule",
                "path": "/sitecore/system/Settings/Rules/Item Saved/Rules/Ensure name",
                "allowedPushOperations": "CreateUpdateAndDelete"
            },                   
            {
                "name": "languages",
                "path": "/sitecore/system/Languages",
                "scope": "DescendantsOnly",
                "allowedPushOperations": "CreateOnly",
                "rules": [
                    {
                        "path": "/en",
                        "scope": "ignored"
                    }
                ]
            }
        ]
    }
}